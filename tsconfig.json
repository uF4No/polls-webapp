{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "types": ["@nuxt/types", "@types/node"],
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ES2020"
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
